<h1>Manage Users</h1>

<p> <%= link_to 'Create New User', admin_new_user_path  %></p>

<% if @user_item.any? %>
  <ul>
    <% @user_item.each do |user| %>
      <li><%=link_to user.name, admin_edit_user_path(user.id) %></li>
      <p><%= user.email %> </p>
      <p><%= user.Role %> </p>
    <% end %>
  </ul>
<% else %>
  <p>No users found.</p>
<% end %>
